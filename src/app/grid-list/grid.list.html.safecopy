<mat-grid-list cols="4" rowHeight="8vh">
  <mat-grid-tile
      *ngFor="let tile of tiles"
      [colspan]="tile.cols"
      [rowspan]="tile.rows"
      [style.background]="tile.color">
    <h2>MealTracker</h2>
  </mat-grid-tile>
  <mat-grid-tile
  [colspan]=4
  [rowspan]=2
  [style.background]=#121212>
<h2>This is a test</h2>
</mat-grid-tile>
  <mat-grid-tile [colspan] = 1 [rowspan] = 1><button mat-raised-button>Basic</button></mat-grid-tile>
</mat-grid-list>
